clearscreen

to square :size
    pu
    fd :size / 2
    lt 90
    pd
    fd :size / 2
    repeat 3 [lt 90 fd :size]
    lt 90
    fd :size / 2
    pu
    rt 90
    bk :size / 2
    pd
end

to circle :size
    pu
    fd :size / 2
    lt 90
    pd
    repeat 360 [ fd :size/(360/3.14159) lt 1]
    rt 90
    pu
    bk :size / 2
    pd
end

square 50
square 100
square 150
circle 75
