; this is an example logo file

repeat 1 [

clearscreen

to square :size
    pu
    fd :size / 2
    lt 90
    setbg :size * 6
    pd
    fd :size / 2
    repeat 3 [lt 90 fd :size]
    lt 90
    fd :size / 2
    pu
    setbg 42
    rt 90
    bk :size / 2
    pd
    stop
end

to tri :size
    pu
    make "sucks :size * 3
    repeat 10 [ for [i 0 :size :sucks] [ fd 10 rt 10 ]]
    pd
    setscrunch 1.001 :sucks + 0.25
end

tri 26

setbg #
setpensize 3.0

square 50
square 100
square 200

for [b 0 10] [tri :b+6]
for [c 90 20] [tri :c-3]
for [a 2 7 1.5] [repeat 5 [fd 10 rt :a-2]]

tri #
forever [square #]

]

